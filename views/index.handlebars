<div>
    <div class='row'>
        <div class='col s3'></div>
        <h5 class='card z-depth-3 center-align col s6'>Click <a href='/scrape'>Scrape</a> to get the latest
            articles</h5>
    </div>
</div>
<div class='row'>
    <div class='col s12'>
        <div class='sol s8 center-align'>
            {{#each article}}
            <div class='card-panel'>
                <h4>{{this.title}}</h4>
                <a href='/readArticle/{{this._id}}' class='waves-effect waves-light btn' id='btnSignUp'>Read
                    Articles</a>
            </div>
            {{/each}}
        </div>
    </div>
</div>